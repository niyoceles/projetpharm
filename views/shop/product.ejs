<%- include('../layouts/header'); -%>

<% result.forEach((item) => { %>
<ul class="list-unstyled">
        <li class="media">
          <img src="images/<%= item.image %>" class="mr-3" alt="..." style="width: 20%; height: 30%;">
          <div class="media-body">
            <h5 class="mt-0 mb-1"><%= item.title %></h5>
            <p><%= item.description %></p>
            <p class="font-italic"><%= item.type %></p>
          </div>
        </li>
    </ul>
    <% }); %>

<%- include('../layouts/header'); -%>